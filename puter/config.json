{
  "name": "Puter",
  "version": "0.1.1",
  "slug": "puter",
  "description": "Self-hosted Puter inside Home Assistant",
  "arch": ["amd64", "aarch64"],
  "startup": "application",
  "boot": "auto",
  "ports": {
    "4100/tcp": 4100
  },
  "map": [
    "config:rw",
    "media:rw",
    "share:rw",
    "addons:rw"
