{
  "name": "Authentik",
  "version": "2024.10.3",
  "slug": "authentik",
  "description": "Self-hosted identity provider with support for OIDC, SAML, and LDAP",
  "arch": ["aarch64", "amd64", "armv7"],
  "init": false,
  "startup": "services",
  "boot": "auto",
  "ports": {
    "9000/tcp": 9000,
    "9443/tcp": 9443
  },
  "ports_description": {
    "9000/tcp": "Authentik HTTP interface",
    "9443/tcp": "Authentik HTTPS interface"
  },
  "ingress": true,
  "ingress_port": 9000,
  "ingress_stream": true,
  "panel_icon": "mdi:shield-account",
  "options": {
    "secret_key": "",
    "admin_email": "admin@example.com",
    "admin_password": "",
    "postgres_password": "",
    "log_level": "info",
    "workers": 2
  },
  "schema": {
    "secret_key": "password?",
    "admin_email": "email",
    "admin_password": "password?",
    "postgres_password": "password?",
    "log_level": "list(debug|info|warning|error)?",
    "workers": "int(1,8)?"
  },
  "map": [
    "share:rw"
  ]
}
