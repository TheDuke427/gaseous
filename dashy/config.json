{
  "name": "Dashy",
  "version": "1.0.0",
  "slug": "dashy",
  "description": "A self-hostable personal dashboard",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "ports": {
    "8080/tcp": 4000
  },
  "ports_description": {
    "8080/tcp": "Web interface"
  },
  "map": ["config:rw"],
  "webui": "http://[HOST]:[PORT:4000]",
  "panel_icon": "mdi:view-dashboard"
}
