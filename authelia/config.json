{
  "name": "Authelia",
  "version": "1.0.0",
  "slug": "authelia",
  "description": "Authentication and authorization server with OIDC support for Cloudflare Zero Trust",
  "arch": ["aarch64", "amd64", "armv7"],
  "startup": "services",
  "boot": "auto",
  "ports": {
    "9091/tcp": 9091
  },
  "ports_description": {
    "9091/tcp": "Authelia web interface"
  },
  "ingress": true,
  "ingress_port": 9091,
  "panel_icon": "mdi:shield-lock",
  "options": {
    "jwt_secret": "",
    "session_secret": "",
    "encryption_key": "",
    "authelia_domain": "auth.yourdomain.com",
    "root_domain": "yourdomain.com",
    "cloudflare_team": "",
    "users": [
      {
        "username": "admin",
        "password": "",
        "email": "admin@example.com",
        "displayname": "Administrator"
      }
    ]
  },
  "schema": {
    "jwt_secret": "password?",
    "session_secret": "password?",
    "encryption_key": "password?",
    "authelia_domain": "str",
    "root_domain": "str",
    "cloudflare_team": "str",
    "users": [
      {
        "username": "str",
        "password": "password",
        "email": "email",
        "displayname": "str?"
      }
    ]
  },
  "map": [
    "share:rw",
    "config:rw"
  ]
}
